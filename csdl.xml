<?xml version='1.0' encoding='UTF-8'?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="OData.PetStore">
              <EntityType Name="Order">
                <Key>
                    <PropertyRef Name="Id"/>
                </Key>
                <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
                <Property Name="PetId" Type="Edm.Int64" Nullable="false"/>
                <Property Name="CustomerId" Type="Edm.Int64" Nullable="false"/>
                <Property Name="ShipDate" Type="Edm.DateTimeOffset" Nullable="false"/>
                <Property Name="Price" Type="Edm.Double"/>
                <Property Name="Status" Type="OData.PetStore.OrderStatus"/>
            </EntityType>
            <EntityType Name="Customer">
              <Key>
                  <PropertyRef Name="Id"/>
              </Key>
              <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
              <Property Name="FirstName" Type="Edm.String" Nullable="false" MaxLength="255"/>
              <Property Name="LastName" Type="Edm.String" Nullable="false" MaxLength="255"/>
              <Property Name="Address" Type="OData.PetStore.Address" Nullable="false"/>
            </EntityType>
            <ComplexType Name="Address">
              <Property Name="Street" Type="Edm.String" MaxLength="255" Nullable="false"/>
              <Property Name="City" Type="Edm.String" MaxLength="255" Nullable="false"/>
              <Property Name="State" Type="Edm.String" MaxLength="255" Nullable="false"/>
              <Property Name="Zip" Type="Edm.String" MaxLength="255" Nullable="false"/>
          </ComplexType>
          <EntityType Name="Category">
            <Key>
                <PropertyRef Name="Id"/>
            </Key>
            <Property Name="Name" Type="Edm.String" Nullable="false" MaxLength="255"/>
            <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
        </EntityType>
          <EntityType Name="Tag">
              <Key>
                  <PropertyRef Name="Id"/>
              </Key>
              <Property Name="Name" Type="Edm.String" Nullable="false" MaxLength="255"/>
              <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
          </EntityType>
          <ComplexType Name="Inventory">
            <Property Name="Animal" Type="Edm.String" Nullable="false" MaxLength="255"/>
            <Property Name="Quantity" Type="Edm.Int64" Nullable="false"/>
          </ComplexType>
          <EntityType Name="Pet">
            <Key>
                <PropertyRef Name="Id"/>
            </Key>
            <Property Name="Id" Type="Edm.Int64" Nullable="false"/>
            <Property Name="Name" Type="Edm.String" MaxLength="255" Nullable="false"/>
            <Property Name="Category" Type="OData.PetStore.Category" Nullable="false"/>
            <Property Name="Tags" Type="Collection(OData.PetStore.Tag)" Nullable="false"/>
            <Property Name="PhotoUrls" Type="Collection(OData.PetStore.String)" Nullable="false" MaxLength="255"/>
            <Property Name="PetStatus" Type="OData.PetStore.PetStatus" Nullable="false"/>
        </EntityType>
            <EnumType Name="OrderStatus" IsFlags="false" UnderlyingType="Edm.Int32">
                <Member Name="placed" Value="0"/>
                <Member Name="approved" Value="1"/>
                <Member Name="delivered" Value="2"/>
                <Member Name="completed" Value="3"/>
            </EnumType>
            <EnumType Name="PetStatus" IsFlags="false" UnderlyingType="Edm.Int32">
                <Member Name="available" Value="0"/>
                <Member Name="pending" Value="1"/>
                <Member Name="sold" Value="2"/>
            </EnumType>

            <EntityContainer Name="OData.PetStoreContainer">
                <EntitySet Name="Orders" EntityType="OData.PetStore.Order">
                </EntitySet>
                <EntitySet Name="Pets" EntityType="OData.PetStore.Pet">
                </EntitySet>
                <EntitySet Name="Customers" EntityType="OData.PetStore.Customer"/>
                <EntitySet Name="Categories" EntityType="OData.PetStore.Category">
                </EntitySet>
                <EntitySet Name="Tags" EntityType="OData.PetStore.Tag"/>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>