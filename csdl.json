{
    "$Version": "4.01",
    "$EntityContainer": "OData.PetStore.Container",
    "OData.PetStore": {
        "Pet": {
            "$Kind": "EntityType",
            "$Key": [
                "id"
            ],
            "id": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "name": {
                "$Type": "Edm.String",
                "$Nullable": false,
                "$MaxLength":255
            },
            "category": {
                "$Type": "OData.PetStore.Category",
                "$Nullable": false
            },
            "photoUrls": {
                "$Type": "Edm.String",
                "$Collection": true,
                "$Nullable": false,
                "$MaxLength":255
            },
            "tags": {
                "$Type": "OData.PetStore.Tag",
                "$Collection": true,
                "$Nullable": false
            },
            "status": {
                "$Type": "OData.PetStore.petStatus",
                "$Nullable": false
            }
        },
        "Order": {
            "$Kind": "EntityType",
            "$Key": [
                "id"
            ],
            "id": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "petId": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "customerId": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },           
            "quantity": {
                "$Type": "Edm.Int32",
                "$Nullable": false
            },
            "shipDate": {
                "$Type": "Edm.DateTimeOffset"
            },
            "status": {
                "$Type": "OData.PetStore.orderStatus"
            },
            "price": {
                "$Type": "Edm.Double",
                "$Nullable": false
            }
            
        },
        "Customer": {
            "$Kind": "EntityType",
            "$Key": [
                "id"
            ],
            "id": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "firstName": {
                "$Type": "Edm.String",
                "$Nullable": false,
                "$MaxLength": 255
            },
            "lastName": {
                "$Type": "Edm.String",
                "$Nullable": false,
                "$MaxLength": 255
            },           
            "address": {
                "$Type": "OData.PetStore.Address",
                "$Nullable": false,
                "$Collection": true
            } 
        },
        "Address": {
            "$Kind": "ComplexType",
            "street": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            },
            "city": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            },
            "state": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            },
            "zip": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            }
        },
        "Category": {
            "$Kind": "EntityType",
            "$Key": [
                "id"
            ],
            "id": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "name": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            }
        },
        "Tag": {
            "$Kind": "EntityType",
            "$Key": [
                "id"
            ],
            "id": {
                "$Type": "Edm.Int64",
                "$Nullable": false
            },
            "name": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            }
        },
        "Inventory": {
            "$Kind": "ComplexType",
            "animal": {
                "$Type": "Edm.String",
                "$Nullable":false,
                "$MaxLength": 255
            },
            "quantity": {
                "$Type": "Edm.Int64",
                "$Nullable":false
            }
        },
        "orderStatus": {
            "$Kind": "EnumType",
            "$IsFlags": false,
            "$UnderlyingType": "Edm.Int32",
            "placed": "0",
            "approved": "1",
            "delivered": "2",
            "completed": "3",
            "$Nullable": "False"
        },
        "petStatus": {
            "$Kind": "EnumType",
            "$IsFlags": false,
            "$UnderlyingType": "Edm.Int32",
            "Available": "0",
            "Pending": "1",
            "Sold": "2"
        },
        "Container": {
            "$Kind": "EntityContainer",
            "Pets": {
                "$Collection": true,
                "$Type": "OData.PetStore.Pet"
            },
            "Orders": {
                "$Collection": true,
                "$Type": "OData.PetStore.Order"
            },
            "Customers": {
                "$Collection": true,
                "$Type": "OData.PetStore.Customer"
            },

            "Tags": {
                "$Collection": true,
                "$Type": "OData.PetStore.Tag"
            },
            "Categories": {
                "$Collection": true,
                "$Type": "OData.PetStore.Category"
            },
            "Inventory": {
                "$Collection": true,
                "$Type": "OData.PetStore.Inventory"
            }
        }
    }
}

